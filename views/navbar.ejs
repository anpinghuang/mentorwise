<style>
  /* NAVIGATION BAR */
  .navbar {
    background-color: rgb(54, 54, 54);
    overflow: hidden;
    text-align: center;
  }

  .navbar a {
    display: inline-block;
    color: white;
    text-align: center;
    padding: 14px 16px;
    margin-left: 30px;
    margin-right: 30px;
    text-decoration: none;
  }

  .navbar a:hover {
    background-color: #ddd;
    color: black;
  }

  .logo {
    display: inline-block;
    vertical-align: middle;
  }

  .logo img {
    width: 300px;
    height: auto;
    margin: 10px;
  }

  /* Hamburger Icon and mobile stuff */
  .icon {
    display: none;
    color: white;
    cursor: pointer;
    font-size: 24px;
  }

  /* Responsive Navigation Bar */
  @media screen and (max-width: 600px) {
    .navbar a {
      display: none;
      width: 100%;
      text-align: center;
    }

    .icon {
      display: block;
      padding-left: 10px;
    }
  }
</style>




<!-- Navigation Bar -->
<div class="navbar">
    <a href="#home">Home</a>
    <a href="#about">About</a>
    <div class="logo">
      <img src="/images/logo_white.png" alt="Your Logo">
    </div>
    <a href="#services">Services</a>
    <a href="#contact">Contact</a>
    <div class="icon" onclick="toggleNavbar()">&#9776;</div>
</div>
<!-- END OF NAVIGATION BAR-->

<script>
    // function for toggling navbars on mobile
    function toggleNavbar() {
        var navLinks = document.querySelectorAll('.navbar a');
        for (var i = 0; i < navLinks.length; i++) {
            navLinks[i].style.display = (navLinks[i].style.display === 'block') ? 'none' : 'block';
        }
    }
  </script>